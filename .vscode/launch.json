    "version": "0.2.0",
    "configurations": [
        {
            "name": "EpgDataCap_Bon GDB Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Debug/EpgDataCap_Bon.elf",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/Debug/",
            "environment": [{
                "name": "LD_PRELOAD",
                "value": "/usr/lib/x86_64-linux-gnu/libasan.so.6.0.0"
            }],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "gdb の再フォーマットを有効にする",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "逆アセンブリ フレーバーを Intel に設定",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
